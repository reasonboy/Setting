<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ZhSeekBar">
        <attr name="enable_color" format="color"/>
        <attr name="disable_color" format="color"/>
        <attr name="bg_color" format="color"/>
        <attr name="thumb_size" format="dimension"/>
        <attr name="bg_width" format="dimension"/>
        <attr name="bg_height" format="dimension"/>
        <attr name="touchable_height" format="dimension"/>
        <attr name="max_value" format="integer"/>
        <attr name="min_value" format="integer"/>
        <attr name="default_value" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ZhCheckBox">
        <attr name="res_off" format="reference"/>
        <attr name="res_on" format="reference"/>
        <attr name="designated_width" format="dimension"/>
        <attr name="designated_height" format="dimension"/>
    </declare-styleable>
</resources>