<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.jzzh.setting.SettingSubItem
        android:id="@+id/setting_display_si_default"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/activity_marginTop"
        app:title="@string/setting_display_si_default"/>

    <com.jzzh.setting.SettingSubItem
        android:id="@+id/setting_display_si_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:title="@string/setting_display_si_user"/>

</LinearLayout>