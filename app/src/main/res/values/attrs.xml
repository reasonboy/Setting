<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingItem">
        <attr name="title" format="string"/>
        <attr name="icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SettingSubItem">
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="TitleLayout">
        <attr name="title" />
        <attr name="function_icon_1" format="reference"/>
        <attr name="function_icon_2" format="reference"/>
        <attr name="enable_checkBox" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AdjustLayout">
        <attr name="icon" />
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="TimeSettingItem">
        <attr name="title" />
        <attr name="summary" format="string"/>
        <attr name="function">
            <enum name="set_time" value="0"/>
            <enum name="checkbox" value="1"/>
            <enum name="set_zone" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="UserImageSettingItem">
        <attr name="describe" format="string"/>
    </declare-styleable>

</resources>